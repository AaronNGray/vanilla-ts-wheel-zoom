{
    "name": "vanilla-js-wheel-zoom",
    "version": "6.10.1",
    "main": "src/wheel-zoom.js",
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/preset-env": "^7.15.6",
        "babel-minify": "^0.5.1",
        "prettier": "2.4.1",
        "rollup": "^2.58.0",
        "rollup-plugin-babel": "^4.4.0"
    },
    "scripts": {
        "build": "yarn rollup && yarn prettier && yarn minify",
        "watch": "yarn rollup --watch",
        "rollup": "node ./node_modules/rollup/dist/bin/rollup --config",
        "prettier": "node ./node_modules/prettier/bin-prettier.js --write dist/wheel-zoom.js",
        "minify": "node ./node_modules/babel-minify/bin/minify.js dist/wheel-zoom.js --out-file dist/wheel-zoom.min.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/worka/vanilla-js-wheel-zoom.git"
    },
    "keywords": [
        "wheelzoom",
        "js",
        "javascript",
        "vanilla",
        "mousewheel",
        "image zoom",
        "image scale",
        "drag scrollable image",
        "zoom",
        "pinch-to-zoom"
    ],
    "author": "worka",
    "license": "GPLv3",
    "bugs": {
        "url": "https://github.com/worka/vanilla-js-wheel-zoom/issues"
    },
